		<AllowedValues>
			<Value>TBD</Value>
			<Value>TBD</Value>
		</AllowedValues>
	</Constraint>
	<Constraint name="Action">
		<AllowedValues>
			<Value>Add</Value>
			<Value>Delete</Value>
		</AllowedValues>
	</Constraint>
</Operation>
